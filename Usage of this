package src1;

public class Instance {

	public static void main(String[] args) {
		//메소드 A랑 B 인스턴스화 하여 새롭게 출력하기
		
		InstanceCopy f1= new InstanceCopy("--------"); //이는 새로운 인스턴스를 생설할 때, 매개변수를 f1에 담는다.  	
		f1.A();
		f1.A();
		
		InstanceCopy f2= new InstanceCopy("********");		
		f2.B();
		f2.B();
		

	}

}
